apt-get update
apt-get install g++
apt-get install make
apt-get install zlib1g-dev
apt-get install libncurses5-dev libncursesw5-dev
wget https://github.com/trinityrnaseq/trinityrnaseq/releases
cd trinityrnaseq-2.2.0
make





