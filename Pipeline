s3cmd get s3://bioinfobiobureau/input/Pipergaudichaudia_S45_L008_R*_001.fastq.gz
s3cmd get s3://bioinfobiobureau/input/Myrceugeniamyrcio_S46_L008_R*_001.fastq.gz
